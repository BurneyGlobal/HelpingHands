<div class="lists">
  <div class="list-card" style="margin-left: 95px;">
    <div class="list-card-header">TODO</div>
    <ul>
      <% @unassigned_tasks.each do |unassigned_task| %>
        <li>
          <%= link_to event_task_path(unassigned_task.event_id, unassigned_task) do %>
          <%= unassigned_task.name %>
          <% end %>
        </li>
      <% end %>
    </ul>
    <div class="list-card-footer">
      <%= link_to '#', "data-toggle" => "modal", "data-target"=>"#myModal" do %>
        <i class="fas fa-plus"></i> add task
      <% end %>
    </div>
  </div>
